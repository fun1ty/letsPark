<script>
    async function enrollReview(id) {
        const result = await axios({
            method: 'POST',
            url: '/shareparking/detail/reviews',
            data : {
                shareParkId : id,
                score : document.getElementById('score').value,
                comment : document.getElementById('comment').value,
            },
            headers: {
                Authorization: `${localStorage.getItem('token')}`,
            },
        })
        console.log(result);

        if(result) {
            alert('Î¶¨Î∑∞Í∞Ä Ï†ïÏÉÅÏ†ÅÏúºÎ°ú ÏûëÏÑ±ÎêòÏóàÏäµÎãàÎã§.');
            document.location.href = `/shareparking/detail?id=${id}`;
        }
    }

    const amVal = Number(document.getElementById('amVal').value);
    document.getElementById('plus').addEventListener('click', ()=> {
        const count = document.getElementById('count');
        const amount = document.getElementById('amount');
        const time = document.getElementById('time');

        count.value = Number(count.value) + 1;
        let mul = Number(count.value) * 30;
        let t = 0;
        while(mul > 30) {
            mul -= 60;
            t++;
        }
        if(mul === 30)
            time.innerText = `${t}ÏãúÍ∞Ñ 30Î∂Ñ`;
        else
            time.innerText = `${t}ÏãúÍ∞Ñ`;

        amount.value = (Number(amount.value.replace(',', '')) + 3000).toLocaleString('ko-KR');
    });
    document.getElementById('minus').addEventListener('click', ()=> {
        const count = document.getElementById('count');
        const amount = document.getElementById('amount');
        const time = document.getElementById('time');

        if(count.value > 0) {
            count.value = Number(count.value) - 1;
            let mul = Number(count.value) * 30;
            let t = 0;
            while(mul > 30) {
                mul -= 60;
                t++;
            }
            if(mul === 30)
                time.innerText = `${t}ÏãúÍ∞Ñ 30Î∂Ñ`;
            else if(mul === 0)
                time.innerText = `0ÏãúÍ∞Ñ 0Î∂Ñ`;
            else
                time.innerText = `${t}ÏãúÍ∞Ñ`;
            amount.value = (Number(amount.value.replace(',', '')) - 3000).toLocaleString('ko-KR');
        }
    });
</script>
